{
	"name": "react-component-generator",
	"displayName": "React Component Generator",
	"description": "",
	"icon": "icon.png",
	"version": "0.1.0",
	"publisher": "alxclark",
	"author": {
		"name": "Alexandre Clark"
	},
	"homepage": "https://github.com/alxclark/ReactComponentGenerator-vscode/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/alxclark/ReactComponentGenerator-vscode"
	},
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.generateReactComponent"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.generateReactComponent",
				"title": "Generate React Component",
				"category": "Files"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.generateReactComponent"
				}
			],
			"editor/context": [
				{
					"command": "extension.generateReactComponent"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
